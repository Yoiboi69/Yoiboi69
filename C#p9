using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace InterfaceExample
{
    interface in1
    {
        void square(int x);
        void fact(int x);
    }

    public class Program : in1
    {
        int z = 1;
        public void square(int x)
        {
            Console.WriteLine("Square of " + x + " is " + x * x);
        }

        public void fact(int x)
        {
            for (int y = 1; y < x; y++)
            {
                z = z * y;
            }
            Console.WriteLine("Factorial of {0} is {1}", x, z);
        }
        static void Main(string[] args)
        {
            Program P2 = new Program();
            P2.square(35);
            P2.fact(10);
            Console.ReadKey();
        }
    }
}
